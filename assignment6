public class Eternitymain {
	/**
	 * @param args
	 */
	public static void main(String[] args) {

//cau1
		Scanner scanner = new Scanner (System.in);
		System.out.println("vui long mot so nguyen");
		while (true) {
			int number = scanner.nextInt();
			if (number > 0) {
				System.out.println("day la so duong");
			}else {
				System.out.println("day la so am");
			}
			if (number % 2 == 0) {
				System.out.println("day la so chan");
			}else {
				System.out.println("day la so le");
			}
			while (true) {
				for (int b = 2; b < 7; b ++) {
					if ( number <= 0) {
						System.out.println("day khong phai la so nguyen to");
						break;
					}else if ( (number % b) == 0 && number != b /*neu chia cho chinh no du 0*/ ) {
					System.out.println("day khong phai la so nguyen to");
					break;
		            }else {
		    			System.out.println("day la so nguyen to");
		    			break;
		            }
				}
				break;
			}
		}
//cau2
		Scanner scanner = new Scanner (System.in);
		List<String> danhSach = new ArrayList<String>();
		System.out.println("vui long mot so nguyen");
		int num = scanner.nextInt();
		danhSach.add("khong");
		danhSach.add("mot");
		danhSach.add("hai");
		danhSach.add("ba");
		danhSach.add("bon");
		danhSach.add("nam");
		danhSach.add("sau");
		danhSach.add("bay");
		danhSach.add("tam");
		danhSach.add("chin");
		danhSach.add("muoi");
		if (num > 10 || num < 1) {
			System.out.println("vui long nhap so tu 1 den 10");
		}
		for (int a = 0;a < 11;a ++) {
			if ( a == num ) {
				System.out.println(danhSach.get(a));
				break;
			}
		}
//cau3
		Scanner scanner = new Scanner (System.in);
		List<Integer> danhSach = new ArrayList<Integer>();
		System.out.println("vui long mot so nguyen (nhan 0 neu ban muon dung va tinh tong)");
		int sum = 0;
		while (true) {
			int num = scanner.nextInt();
			if (num == 0) {
				System.out.println(sum);
				break;
			}
            danhSach.add(num);
            sum += num;
		}
	}
}
